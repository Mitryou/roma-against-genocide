{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nav": {
      "type": "component",
      "repeatable": true,
      "component": "nav.navigation"
    },
    "timeline": {
      "type": "component",
      "repeatable": true,
      "component": "date-components.timeline"
    },
    "sourceList": {
      "type": "component",
      "repeatable": true,
      "component": "sources.list"
    },
    "readMore": {
      "type": "component",
      "repeatable": false,
      "component": "read-more.read-more"
    },
    "titleArticle": {
      "type": "string",
      "required": true,
      "default": "title-link-example"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "date-components.date-numbers",
        "nav.anchor",
        "text-elements.h1",
        "text-elements.h2",
        "text-elements.h3",
        "text-elements.p",
        "text-elements.right-image",
        "images.images"
      ]
    },
    "timeToRead": {
      "type": "biginteger",
      "required": true,
      "default": "1"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "shortDesc": {
      "type": "string",
      "required": true,
      "default": "Lorem ipsum is placeholder text commonly used in the graphic, print, and publishing industries for previewing."
    }
  }
}
